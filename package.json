{
  "description": "Personal homepage.",
  "author": "Moritz Rehbach",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite --force",
    "dev:expose": "vite --host",
    "debug": "DEBUG=vite:* vite",
    "build": "npm run lint && vite build",
    "lint": "tsc && npx eslint --max-warnings=0",
    "lint:fix": "tsc && npx eslint --fix src",
    "preview": "vite build && vite preview",
    "preview:expose": "vite build && vite preview --host",
    "prepare": "husky install"
  },
  "dependencies": {
    "@geist-ui/core": "2.3.8",
    "@geist-ui/icons": "1.0.2",
    "lodash-es": "4.17.21",
    "normalize-url": "8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-spotify-embed": "2.0.3",
    "react-streaming": "0.3.5",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@types/eslint": "8.44.8",
    "@types/lodash-es": "4.17.12",
    "@types/node": "18.15.5",
    "@types/react": "18",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vitejs/plugin-react": "3.1.0",
    "clsx": "2.0.0",
    "eslint": "8.54.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "postcss-nesting": "12.0.1",
    "postcss-preset-env": "9.3.0",
    "terser": "5.16.8",
    "typescript": "5.2.2",
    "typescript-plugin-css-modules": "5.0.2",
    "vike": "0.4.147",
    "vite": "4.2.1",
    "vite-imagetools": "4",
    "vite-plugin-cjs-interop": "2.3.0"
  },
  "overrides": {
    "sharp": "0.33"
  }
}
